{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-console": "warn",
    "no-unused-vars": "error",
    "no-eval": "error",
    "no-new-func": "error",
    "no-use-before-define": "error"
  }
}
